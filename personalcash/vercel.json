{

  "version": 2,

  "builds": [

    {

      "src": "package.json",

      "use": "@vercel/static-build",

      "config": {

        "distDir": "dist" // Diretório de saída após o export do Expo

      }

    }

  ],

  "routes": [

    {

      "src": "/(.*)",

      "dest": "/dist/$1"

    }

  ]

}
