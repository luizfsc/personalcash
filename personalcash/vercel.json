{

  "version": 2,

  "builds": [

    {

      "src": "package.json",

      "use": "@vercel/static-build",

      "config": {

        "distDir": "web-build" // Diretório de saída padrão do Expo para web builds

      }

    }

  ],

  "routes": [

    {

      "src": "/(.*)",

      "dest": "/web-build/$1"

    }

  ]

}
